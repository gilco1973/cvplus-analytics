{
  "name": "@cvplus/analytics",
  "version": "1.0.0",
  "description": "CVPlus Analytics Module - Advanced analytics, revenue tracking, and business intelligence",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.esm.js",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist/**/*",
    "src/**/*",
    "README.md"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "type-check": "tsc --noEmit",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "lint": "eslint src --ext .ts,.tsx",
    "lint:fix": "eslint src --ext .ts,.tsx --fix"
  },
  "dependencies": {
    "@cvplus/core": "file:../core",
    "firebase": "^10.7.0",
    "firebase-admin": "^12.0.0",
    "firebase-functions": "^5.0.1",
    "ioredis": "^5.3.0",
    "stripe": "^15.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "tsup": "^8.0.0",
    "typescript": "^5.3.0",
    "vitest": "^1.0.0",
    "eslint": "^8.56.0",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0"
  },
  "keywords": [
    "cvplus",
    "analytics",
    "revenue",
    "metrics",
    "business-intelligence",
    "cohort-analysis"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:gilco1973/cvplus-analytics.git"
  },
  "author": "Gil Klainert",
  "license": "MIT"
}